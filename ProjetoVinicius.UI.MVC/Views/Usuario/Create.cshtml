@model ProjetoVinicius.Domain.Entities.Usuario

@{
    ViewBag.Title = "Create";
}
<h4>Usuario</h4>
<hr />


<form method="post" id="form">
    <div class="form-horizontal">

        <div class="form-group">
            <label class="radio-inline">
                Nome:
                <input type="text" name="nome" value="@Model.Nome" class="form-control" />
            </label>
        </div>
        <div class="form-group">
            <label class="radio-inline">
                cpf:
                <input type="text" name="cpf" value="@Model.Senha" class="form-control" />
            </label>
        </div>
        <div class="form-group">
            <label class="radio-inline">
                cep:
                <input type="text" name="cep" value="@Model.Email" class="form-control" />
            </label>
        </div>
        <div class="form-group">
            <label class="radio-inline">
                Rua:
                <input type="text" name="rua" value="@Model.Removido" class="form-control" />
            </label>
        </div>
        <div class="form-group">
            <label class="radio-inline">
                Data:
                <input type="text" name="Data" value="@Model.Data" class="form-control" />
            </label>
        </div>

        <div>
            @Html.ActionLink("Voltar", "Index")
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Enviar" class="btn btn-default" />
            </div>
        </div>
    </div>
</form>

